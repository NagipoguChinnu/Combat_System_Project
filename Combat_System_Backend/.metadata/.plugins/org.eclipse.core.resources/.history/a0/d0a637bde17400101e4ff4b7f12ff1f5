package com.combatsystem.www.model;

import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToOne;

@Entity
public class UavEncryptedMessage {

	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private Long id;
	
	private String latitudeBlock1;
	private String latitudeBlock2;
	private String latitudeBlock3;
	
	private String longitudeBlock1;
	private String longitudeBlock2;
	private String longitudeBlock3;

	private String block1;
    private String block2;
    private String block3;
    private String status="OK";
    
    @ManyToOne
	@JoinColumn(name = "user_id") // This connects to UAVOperator.id
	private UAVOperator uavOperator;
    
	public UAVOperator getUavOperator() {
		return uavOperator;
	}
	public void setUavOperator(UAVOperator uavOperator) {
		this.uavOperator = uavOperator;
	}
	
	public String getLatitudeBlock1() {
		return latitudeBlock1;
	}
	public void setLatitudeBlock1(String latitudeBlock1) {
		this.latitudeBlock1 = latitudeBlock1;
	}
	public String getLatitudeBlock2() {
		return latitudeBlock2;
	}
	public void setLatitudeBlock2(String latitudeBlock2) {
		this.latitudeBlock2 = latitudeBlock2;
	}
	public String getLatitudeBlock3() {
		return latitudeBlock3;
	}
	public void setLatitudeBlock3(String latitudeBlock3) {
		this.latitudeBlock3 = latitudeBlock3;
	}
	public String getLongitudeBlock1() {
		return longitudeBlock1;
	}
	public void setLongitudeBlock1(String longitudeBlock1) {
		this.longitudeBlock1 = longitudeBlock1;
	}
	public String getLongitudeBlock2() {
		return longitudeBlock2;
	}
	public void setLongitudeBlock2(String longitudeBlock2) {
		this.longitudeBlock2 = longitudeBlock2;
	}
	public String getLongitudeBlock3() {
		return longitudeBlock3;
	}
	public void setLongitudeBlock3(String longitudeBlock3) {
		this.longitudeBlock3 = longitudeBlock3;
	}
	
	public String getBlock1() {
		return block1;
	}
	public void setBlock1(String block1) {
		this.block1 = block1;
	}
	public String getBlock2() {
		return block2;
	}
	public void setBlock2(String block2) {
		this.block2 = block2;
	}
	public String getBlock3() {
		return block3;
	}
	public void setBlock3(String block3) {
		this.block3 = block3;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
}
